<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.inventariocmrm">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cmrm_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cmrm_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Equipamentos"></activity>
        <activity android:name=".EquipamentosDetalhes" />
        <activity android:name=".add_agrupamentos" />
        <activity android:name=".ScanCodeActivity" />
        <activity android:name=".add_equip" />
        <activity android:name=".TabsSec" />
        <activity
            android:name=".home"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>